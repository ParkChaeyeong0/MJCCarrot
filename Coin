using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Coin : MonoBehaviour
{
  //Player가 Coin과 닿았을 시 코인 삭제
    private void OnTriggerEnter(Collider c)
    { 
        //Is Trigger 체크하기
        if(c.gameObject.name == "Coin")
        {
            Destroy(gameObject);
        }
    }
}
